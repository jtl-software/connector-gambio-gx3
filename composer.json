{
    "name": "jtl/connector-gambio",
    "type": "app",
    "description": "Gambio Connector based on jtl/connector",
    "keywords": ["jtl", "Connector"],
    "homepage": "http://www.jtl-software.de",
    "license": "GPL",
    "authors": [
        {
            "name": "JTL Software GmbH",
            "email": "info@jtl-software.de",
            "homepage": "http://www.jtl-software.de",
            "role": "Founder"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "jtl/connector": "^3.2",
        "phing/phing": "^2.16",
        "nette/utils": "^3.1",
        "ext-json": "*",
        "jtl/connector-components-xtc": "^0.1.0"
    },
    "require-dev": {
        "jtl/connector-integration-tests": "dev-develop",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpcompatibility/php-compatibility": "dev-develop",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "jtl\\Connector\\Gambio\\": "src/"
        }
    },
    "scripts": {
        "build": [
            "@composer update",
            "./vendor/bin/phpcs src/ --extensions=php -p --standard=phpcs.xml.dist",
            "./vendor/bin/phpstan analyse",
            "@composer update --no-dev",
            "./vendor/bin/phing package",
            "@composer update"
        ]
    }
}
